{
  "name": "di-data-analytics-platform",
  "version": "0.0.1",
  "description": "Data and Analytics platform which will enable the implementation of the OneLogin reporting strategy",
  "license": "MIT",
  "repository": "github:alphagov/di-data-analytics-platform",
  "devDependencies": {
    "@aws-lambda-powertools/logger": "^1.12.1",
    "@aws-lambda-powertools/metrics": "^1.12.1",
    "@aws-sdk/client-athena": "^3.388.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.388.0",
    "@aws-sdk/client-ec2": "^3.388.0",
    "@aws-sdk/client-firehose": "^3.388.0",
    "@aws-sdk/client-lambda": "^3.388.0",
    "@aws-sdk/client-redshift-data": "^3.388.0",
    "@aws-sdk/client-s3": "^3.388.0",
    "@aws-sdk/client-sfn": "^3.388.0",
    "@aws-sdk/client-sqs": "^3.388.0",
    "@aws-sdk/types": "^3.387.0",
    "@faker-js/faker": "^8.0.2",
    "@smithy/util-retry": "^2.0.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "aws-sdk-client-mock": "^3.0.0",
    "esbuild": "^0.19.0",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "jest": "^29.6.2",
    "jest-stare": "^2.5.1",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "build": "scripts/build-lambdas.sh",
    "check": "npm run format:check && npm run lint:check",
    "format:check": "prettier --check src tests/integration-tests",
    "format:fix": "prettier --write src tests/integration-tests",
    "lint:check": "eslint src tests/integration-tests",
    "lint:fix": "eslint --fix src tests/integration-tests",
    "test": "jest --silent src",
    "integration-test": "scripts/integration-test-check.sh && jest --silent ./tests/integration-tests --maxWorkers=4",
    "e2e-test": "scripts/integration-test-check.sh && jest --silent ./tests/e2e-tests --maxWorkers=4",
    "smoke-test": "scripts/integration-test-check.sh && jest --silent ./tests/smoke-tests --maxWorkers=4",
    "iac:build": "scripts/build-sam-template.sh",
    "iac:lint": "npm run iac:build && sam validate --lint --region eu-west-2",
    "iac:scan": "npm run iac:build && checkov --config-file .checkov.local.yml --file template.yaml --quiet",
    "iac:format:check": "prettier --check iac",
    "iac:format:fix": "prettier --write iac",
    "teardown:prepare": "scripts/teardown-prepare.sh",
    "teardown:delete-stack": "sam delete --region eu-west-2 --stack-name dap --no-prompts --debug"
  }
}
